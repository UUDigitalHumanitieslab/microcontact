{{#show}}
	<ol id="tab-participate" class="breadcrumb" role="tab">
		<li><a href="#participate">i18n navParticipateLink</a>
		{{#country}}
		<li><a href="#participate/{{country}}">{{country}}</a>
		{{/country}}
		{{#query}}
		<li><a href="#participate/{{country}}/{{query}}">{{query}}</a>
		{{/query}}
	</ol>
	<ol id="tab-contributions" class="breadcrumb" role="presentation">
		<li><a href="#contributions" role="tab">i18n navContributionsLink</a></li>
	</ol>
{{/show}}
