		<nav id="nav-main"></nav>
		<main></main>
		<script
			{{#production}}
			src="//cdnjs.cloudflare.com/ajax/libs/require.js/2.3.6/require.min.js"
			{{/production}}{{^production}}
			src="static/bower_components/requirejs/require.js"
			{{/production}}
		></script>
		<script>
			{{#production}}
			{{>productionConfig}}
			{{/production}}{{^production}}
			{{>developConfig}}
			{{/production}}
			define('googlemaps', [
				'async!https://maps.google.com/maps/api/js?v=3&libraries=places&key={{gmapikey}}'
			], function() {
				return window.google.maps;
			});
			{{#production}}
			require(['microcontact']);
			{{/production}}{{^production}}
			require(['main']);
			{{/production}}
		</script>
		{{^production}}
		<script src="//localhost:35729/livereload.js"></script>
		{{/production}}
